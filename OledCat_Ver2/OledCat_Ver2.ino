#include <Wire.h>
#include <Adafruit_GFX.h>
#include <Adafruit_SH1106.h>
#include <string.h>

#define NUM_OF_SERIAL_DATA 8
#define SCREEN_WIDTH 128 // OLED display width, in pixels
#define SCREEN_HEIGHT 64 // OLED display height, in pixels

#define OLED_RESET 4
Adafruit_SH1106 display(OLED_RESET);

#if (SH1106_LCDHEIGHT != 64)
#error("Height incorrect, please fix Adafruit_SH1106.h!");
#endif

char c;
short countType = 0;
short loopCycleCount = 0; 
void setup() {
  Serial.begin(115200);
  
  // by default, we'll generate the high voltage from the 3.3v line internally! (neat!)
  display.begin(SH1106_SWITCHCAPVCC, 0x3C);  // initialize with the I2C addr 0x3D (for the 128x64)
  // init done
  
  // Show image buffer on the display hardware.
  // Since the buffer is intialized with an Adafruit splashscreen
  // internally, this will display the splashscreen.
  
  // Clear the buffer.
  display.clearDisplay();
  // display.invertDisplay(true);

  for(int i = 0; i<1; i++)
  {
	display.clearDisplay();
	display.drawBitmap(0, 0, static1, 128, 64, 1);
	display.display();
	delay(300);
	display.clearDisplay();
	display.drawBitmap(0, 0, static2, 128, 64, 1);
	display.display();
	delay(300);
	display.clearDisplay();
	display.drawBitmap(0, 0, static3, 128, 64, 1);
	display.display();
	delay(300);
  }
  display.clearDisplay();
  display.drawBitmap(0, 0, cat1, 128, 64, 1);
  display.display();
}

void loop() {
  while(Serial.available())
  {
	c = Serial.read();
	countType++;
  }
  if(countType > 0)
  {
  	display.clearDisplay();
	display.drawBitmap(0, 0, cat1, 128, 64, 1);
	display.display();
	delay(70);
	display.clearDisplay();
	display.drawBitmap(0, 0, cat2, 128, 64, 1);
	display.display();
	delay(70);
	countType -= NUM_OF_SERIAL_DATA + 1;

	if(countType <= 0)
	{
		countType = 0;
	}
  }


  if(countType == 0)
  {
  	if(loopCycleCount % 900 == 0)
  	{
  		display.clearDisplay();
		display.drawBitmap(0, 0, static1, 128, 64, 1);
		display.display();
		loopCycleCount = 0;
  	}
  	else if (loopCycleCount % 600 == 0)
  	{
  		display.clearDisplay();
		display.drawBitmap(0, 0, static2, 128, 64, 1);
		display.display();
  	}
  	else if (loopCycleCount % 300 == 0)
  	{
  		display.clearDisplay();
  		display.drawBitmap(0, 0, static3, 128, 64, 1);
  		display.display();
  	}
  }
  loopCycleCount++;
}
